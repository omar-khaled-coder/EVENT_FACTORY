<p style="color: green"><%= notice %></p>

<h1>Spaces</h1>

<div id="spaces">
  <% @spaces.each do |space| %>
  <% if space.accepted? %>
    <%= render space %>
    <p>
      <%= link_to "Show this space", space %>
    </p>
     <% end %>
  <% end %>
</div>

<%= link_to "New space", new_space_path %>
